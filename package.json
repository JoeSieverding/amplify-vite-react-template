{
  "name": "amplify-vite-react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && shx cp node_modules/pdfjs-dist/build/pdf.worker.js dist/",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare-pdf-worker": "shx mkdir -p public && shx cp node_modules/pdfjs-dist/build/pdf.worker.js public/",
    "postinstall": "npm run prepare-pdf-worker || exit 0"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.11.1",
    "@aws-sdk/client-bedrock": "^3.799.0",
