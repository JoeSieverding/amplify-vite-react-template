    "@cloudscape-design/global-styles": "^1.0.41",
    "aws-amplify": "^6.0.0",
    "json-schema-to-ts": "^3.1.1",
    "mammoth": "^1.6.0",
    "pdfjs-dist": "3.11.174",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "ts-algebra": "^2.0.0"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.5.0",
    "@aws-amplify/backend-cli": "^1.2.9",
    "@types/babel__generator": "^7.27.0",
